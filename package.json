{
  "name": "teme-formstate",
  "version": "0.0.1",
  "description": "Simple stream-based field/form state",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "clean": "rm -rf coverage/ .nyc_output/ node_modules/.cache/",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"|snazzy",
    "format": "prettier-standard \"src/**/*.js\" \"test/**/*.js\"",
    "build:dev": "npm run lint && microbundle --format es,cjs --no-compress",
    "build:watch": "npm run lint && microbundle watch --format es,cjs --no-compress",
    "build:prod": "npm run clean && npm run lint && microbundle --format es,cjs",
    "test": "npm run clean && npm run lint && nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && http-server coverage/ -p 8000"
  },
  "repository": {
    "type": "git",
    "url": "/nas/config/git/teme-formstate.git/"
  },
  "author": "Alan Layng",
  "license": "Unlicense",
  "devDependencies": {
    "ava": "^1.2.1",
    "esm": "^3.2.5",
    "http-server": "^0.11.1",
    "microbundle": "^0.9.0",
    "nyc": "^13.3.0",
    "prettier-standard": "^9.1.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "teme": "github:ludlovian/teme"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
